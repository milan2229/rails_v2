<h2>Questions</h2>
<div class="row">
  <div class="col-md-12">
    <table class="table table-striped">
      <thead class="table-light">
        <tr>
          <td>Id</td>
          <td>Title</td>
          <td>Menu</td>
        </tr>
      </thead>
      <tbody>
     <%= @questions.each do |question| %>
        <tr>
          <td><%= question.id %></td>
          <td><%= question.title %></td>
          <td>[edit] [delete]</td>
        </tr>
        <% end %>
      </tbody>

    </table>

  </div>
</div>